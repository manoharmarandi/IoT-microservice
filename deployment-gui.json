{
    "modulesContent": {
      "$edgeAgent": {
        "properties.desired": {
          "schemaVersion": "1.0",
          "runtime": {
            "type": "docker",
            "settings": {
              "minDockerVersion": "v1.25",
              "loggingOptions": "",
              "registryCredentials": {
                "your-container-registry": {
                  "username": "your-registry-username",
                  "password": "your-registry-password",
                  "address": "your-container-registry.azurecr.io"
                }
              }
            }
          },
          "modules": {
            "gui": {
              "version": "1.0",
              "type": "docker",
              "status": "running",
              "restartPolicy": "always",
              "settings": {
                "image": "your-container-registry/gui:latest",
                "createOptions": {}
              }
            }
          }
        }
      }
    }
  }
  