{
    "modulesContent": {
      "$edgeAgent": {
        "properties.desired": {
          "schemaVersion": "1.0",
          "runtime": {
            "type": "docker",
            "settings": {
              "minDockerVersion": "v1.25",
              "loggingOptions": "",
              "registryCredentials": {
                "your-container-registry": {
                  "username": "your-registry-username",
                  "password": "your-registry-password",
                  "address": "your-container-registry.azurecr.io"
                }
              }
            }
          },
          "systemModules": {
            "edgeAgent": {
              "type": "docker",
              "settings": {
                "image": "mcr.microsoft.com/azureiotedge-agent:1.2",
                "createOptions": {}
              }
            },
            "edgeHub": {
              "type": "docker",
              "status": "running",
              "restartPolicy": "always",
              "settings": {
                "image": "mcr.microsoft.com/azureiotedge-hub:1.2",
                "createOptions": {}
              }
            }
          },
          "modules": {
            "temperatureDataCollector": {
              "version": "1.0",
              "type": "docker",
              "status": "running",
              "restartPolicy": "always",
              "settings": {
                "image": "your-container-registry/temperature-data-collector:latest",
                "createOptions": {}
              }
            }
          }
        }
      },
      "$edgeHub": {
        "properties.desired": {
          "routes": {
            "temperatureDataCollectorToIoTHub": "FROM /messages/modules/temperatureDataCollector/outputs/* INTO $upstream"
          },
          "schemaVersion": "1.0"
        }
      }
    }
  }
  